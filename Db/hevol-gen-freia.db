# Linde Dewar
##################

record (calc, "$(P):Dwr:VolLHe") {
  field(SCAN, "10 second")
  field(EGU, "m3")
  field(PREC, 3)
  field(INPA, "Cryo-Dwr:LHe:LI5200_Lt")
  field(CALC, "A/1000")
  field(FLNK, "$(P):Dwr:VolGHe")
}
record (calc, "$(P):Dwr:VolGHe") {
  field(EGU, "m3")
  field(PREC, 3)
  field(INPA, "Cryo-Dwr:LHe:LI5200_Lt")
  field(INPB, "$(P):Dwr:Vol")
  field(CALC, "B-A/1000")
}

record (calc, "$(P):Dwr:MassLHe") {
  field(EGU, "kg")
  field(PREC, 1)
  field(SCAN, "10 second")
  field(INPA, "-2e-6")
  field(INPB, "-0.016")
  field(INPC, "143.59")
  field(INPK, "Cryo-CBox:LP:PI3290")
  field(INPL, "$(P):Dwr:VolLHe")
  field(CALC, "(A*(K^2)+B*K+C)*L")
  field(FLNK, "$(P):Dwr:MassGHe")
}

record (calc, "$(P):Dwr:MassGHe") {
  field(EGU, "kg")
  field(PREC, 1)
  field(INPA, "4e-6")
  field(INPB, "0.0099")
  field(INPC, "2.8838")
  field(INPK, "Cryo-CBox:LP:PI3290")
  field(INPL, "$(P):Dwr:VolGHe")
  field(CALC, "(A*(K^2)+B*K+C)*L")
  field(FLNK, "$(P):Dwr:MassHe")
}

record (calc, "$(P):Dwr:MassHe") {
  field(EGU, "kg")
  field(PREC, 1)
  field(INPA, "$(P):Dwr:MassLHe")

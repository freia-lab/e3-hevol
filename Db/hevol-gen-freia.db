# He mass below the lambda plate

record (calc, "$(P):MagIns:MassLHe-1") {
  field(EGU, "kg")
  field(PREC, 1)
  field(SCAN, "10 second")
  field(INPA, "-5.02")
  field(INPB, "21.5")
  field(INPC, "125")
  field(INPK, "CstatV-LHe:TT663M:sRdV")
  field(INPL, "$(P):MagIns:VolLHe-1")
  field(CALC, "(A*K*K+B*K+C)*L")
  field(FLNK, "$(P):MagIns:MassGHe-1")
}

